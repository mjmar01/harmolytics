struct Transaction {
    byte[] hash;
    byte[] ethHash;
    byte[] sender;
    byte[] receiver;
    uint32 blockNum;
    uint64 timeStamp;
    byte[] amount;
    byte[] input;
    Log[]  logs;
    byte   status;
    uint32 gasAmount;
    byte[] gasPrice;
    byte   shard;
    byte   toShard;
}

struct Log {
    uint16 index;
    byte[] address;
    byte[] topics;
    byte[] data;
}